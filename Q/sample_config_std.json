{
    "experiment": {
        "title": "Test - std",
        "ID": 4
    },
    "model": {
        "backbone": "resnet18",
        "config": {
            "algorithm": "std",
            "reduction": 16
        }
    },
    "dataloader": {
        "dataset": "ImageNet",
        "train_loader_params": {
            "batch_size": 256,
            "num_workers": 32,
            "shuffle": true,
            "pin_memory": true
        },
        "eval_loader_params": {
            "batch_size": 256,
            "num_workers": 32,
            "shuffle": false,
            "pin_memory": true
        }
    },
    "optimizer": {
        "type": "SGD",
        "params": {
            "lr": 0.01,
            "momentum": 0.9,
            "weight_decay": 0.0001
        }
    },
    "loss_function": {
        "type": "CrossEntropyLoss"
    },
    "scheduler": {
        "type": "MultiStepLR",
        "params": {
            "milestones": [
                30,
                60
            ],
            "gamma": 0.1
        }
    },
    "gradient_clipping": null,
    "train_constants": {
        "max_epochs": 1,
        "log_train_stats_every_iters": 100,
        "save_every_epochs": 1
    },
    "resume_from": null
}